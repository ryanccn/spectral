{
  "name": "spectral",
  "productName": "Spectral",
  "description": "A elegant and powerful Minecraft launcher for macOS",
  "private": true,
  "version": "0.0.1",
  "author": "Ryan Cao",
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "build": "node scripts/build.js",
    "compile:preview": "cross-env MODE=production yarn run build && electron-builder build --config .electron-builder.config.js -m --arm64 --dir",
    "compile": "cross-env MODE=production yarn run build && electron-builder build --config .electron-builder.config.js -m --arm64 --x64 --universal --zip",
    "dev": "node scripts/dev.js",
    "types:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "types:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "types:renderer": "vue-tsc --noEmit -p packages/renderer/tsconfig.json",
    "types:sync": "dts-cb -i packages/preload/src/**/*.ts -o packages/preload/exposedInMainWorld.d.ts",
    "types": "yarn run types:main && yarn run types:preload && yarn run types:sync && yarn run types:renderer",
    "update-electron-vendors": "node scripts/update-electron-vendors.js"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.2",
    "@types/electron-devtools-installer": "2.2.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.24",
    "@vitejs/plugin-vue": "^2.3.1",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "dts-for-context-bridge": "^0.7.1",
    "electron": "^18.0.3",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "execa": "^6.1.0",
    "got": "^12.0.3",
    "hasha": "^5.2.2",
    "node-fetch": "^3.2.3",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.0.23",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vue-tsc": "^0.34.4"
  },
  "dependencies": {
    "@headlessui/vue": "^1.5.0",
    "@heroicons/vue": "^1.0.6",
    "@vueuse/core": "^8.2.5",
    "electron-updater": "^5.0.1",
    "p-limit": "^4.0.0",
    "pinia": "^2.0.13",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "zod": "^3.14.4"
  }
}
